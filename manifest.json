{
    "name":"Asciidoctor.js Editor",
    "description":"AsciiDoc Editor inside Chrome!",
    "version":"0.0.1",
    "manifest_version":2,

    "icons":{
        "16":"img/asciidoc_icon_16.png",
        "48":"img/asciidoc_icon_48.png",
        "128":"img/asciidoc_icon_128.png"
    },

    "app":{
        "background":{
            "scripts":["background.js"]
        }
    },

    "permissions":[
        "app.window"
    ]
}
